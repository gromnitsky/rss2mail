{
    "name": "rss2mail",
    "version": "1.0.1",
    "description": "Convert an rss/atom feed to a bunch of emails in mbox/rnews compatible output; it does NOT send the emails",
    "type": "module",
    "bin": "rss2mail.js",
    "files": [
        "index.js",
        "rss2mail.js",
        "usage.txt"
    ],
    "keywords": [
        "atom",
        "rss",
        "feed",
        "email",
        "mime",
        "mbox",
        "rnews"
    ],
    "author": "Alexander Gromnitsky",
    "repository": "github:gromnitsky/rss2mail",
    "license": "MIT",
    "dependencies": {
        "grepfeed": "~2.0.0",
        "lockfile": "~1.0.4",
        "nodemailer": "~6.9.4"
    }
}
