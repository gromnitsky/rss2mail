{
    "name": "rss2mail",
    "version": "0.0.4",
    "description": "Convert an rss/atom feed to a bunch of emails in mbox/rnews compatible output; it does NOT send the emails",
    "main": "index.js",
    "bin": "rss2mail",
    "files": [
        "index.js",
        "rss2mail"
    ],
    "keywords": [
        "atom",
        "rss",
        "feed",
        "email",
        "mime",
        "mbox",
        "rnews"
    ],
    "author": "Alexander Gromnitsky",
    "repository": "github:gromnitsky/rss2mail",
    "license": "MIT",
    "dependencies": {
        "emailjs-mime-builder": "~2.0.5",
        "grepfeed": "~1.2.2",
        "lockfile": "~1.0.4",
        "minimist": "~1.2.0"
    },
    "engines": {
        "node": ">=10"
    }
}
